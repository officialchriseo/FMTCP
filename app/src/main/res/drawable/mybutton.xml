<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="35dip" />
            <stroke android:width="1dip" android:color="#ee4e0560" />
            <gradient android:angle="-90" android:startColor="#ee4e0560" android:endColor="#EE9006B3"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="35dip" />
            <stroke android:width="1dip" android:color="#ee4e0560" />
            <solid android:color="#EE9006B3"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="35dip" />
            <stroke android:width="1dip" android:color="#ee4e0560" />
            <gradient android:angle="-90" android:startColor="#eec606f7" android:endColor="#EE9006B3" />
        </shape>
    </item>
</selector>